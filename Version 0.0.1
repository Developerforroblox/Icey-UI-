-- Scripts:

local function BGCEL_fake_script() -- FE.Script 
	local script = Instance.new('Script', FE)

	if game.workspace.filteringEnabled == false
		then script.Parent.Text = "FE is Disabled"
	end
	
	if game.workspace.FilteringEnabled == true
		then script.Parent.Text = "FE is Enabled"
	end
	
end
coroutine.wrap(BGCEL_fake_script)()
local function KAUW_fake_script() -- Exit.LocalScript 
	local script = Instance.new('LocalScript', Exit)

	script.Parent.MouseButton1Click:Connect(function()
		script.Parent.Parent.Visible = false
	end)
end
coroutine.wrap(KAUW_fake_script)()
local function RPNEN_fake_script() -- Frame.draggable 
	local script = Instance.new('Script', Frame)

	script.Parent.Active = true
	script.Parent.Draggable = true
end
coroutine.wrap(RPNEN_fake_script)()
